
type User {
    id: ID! @id
    createdAt: DateTime! @createdAt
    updatedAt: DateTime! @updatedAt
    firstName: String!
    lastName: String!
    email: String! @unique
    password: String!
    notes: [Note]!
}

type Note {
    id: ID! @id
    createdAt: DateTime! @createdAt
    updatedAt: DateTime! @updatedAt
    user: ID!
    title: String!
    body: String!
    isPublished: Boolean @default(value: false)
}
